<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloudhelios.atlantis.budget.persistence.CshBankAccountMapper">

                <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.cloudhelios.atlantis.budget.domain.CshBankAccount">
                                                <id column="id" property="id" />
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                <result column="bank_account_name" property="bankAccountName" />
                                                                <result column="bank_account_num" property="bankAccountNum" />
                                                                <result column="bank_branch_id" property="bankBranchId" />
                                                                <result column="company_id" property="companyId" />
                                                                <result column="city_name" property="cityName" />
                                                                <result column="city_code" property="cityCode" />
                                                                <result column="province_name" property="provinceName" />
                                                                <result column="province_code" property="provinceCode" />
                                                                <result column="cash_account_id" property="cashAccountId" />
                                                                <result column="responsibility_center_id" property="responsibilityCenterId" />
                                                                <result column="version_number" property="versionNumber" />
                                                                <result column="is_enabled" property="isEnabled" />
                                                                <result column="is_deleted" property="isDeleted" />
                                                                <result column="created_date" property="createdDate" />
                                                                <result column="created_by" property="createdBy" />
                                                                <result column="last_updated_date" property="lastUpdatedDate" />
                                                                <result column="last_updated_by" property="lastUpdatedBy" />
                                    </resultMap>

        <!-- 通用分页查询 -->
    <select id="getCshBankAccountByCond" parameterType="com.cloudhelios.atlantis.budget.domain.CshBankAccount" resultMap="BaseResultMap">
        select
                                    id
                                                                                                                                                                                                                                                                                                                                                                                                                                        ,bank_account_name
                                                ,bank_account_num
                                                ,bank_branch_id
                                                ,company_id
                                                ,city_name
                                                ,city_code
                                                ,province_name
                                                ,province_code
                                                ,cash_account_id
                                                ,responsibility_center_id
                                                ,version_number
                                                ,is_enabled
                                                ,is_deleted
                                                ,created_date
                                                ,created_by
                                                ,last_updated_date
                                                ,last_updated_by
                            from csh_bank_account
        <where>
            ${ew.sqlSegment}
        </where>
    </select>
</mapper>
