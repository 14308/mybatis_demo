<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.AtlDepartmentMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.example.demo.entity.AtlDepartment">
		<id column="id" property="id" />
		<result column="department_name" property="departmentName" />
		<result column="code" property="code" />
		<result column="departemtn_status" property="departemtnStatus" />
		<result column="createdDate" property="createdDate" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, department_name AS departmentName, code, departemtn_status AS departemtnStatus, createdDate
    </sql>

</mapper>
